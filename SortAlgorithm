def BubbleSort(sortlist):
    for y in range(len(sortlist)):
        isChange = False
        for i in range(len(sortlist)-y-1):
            if sortlist[i] > sortlist[i+1] :
                isChange = True
                sortlist[i], sortlist[i+1] = sortlist[i+1],sortlist[i]
        print(sortlist,y)
        if not isChange:
            break #or return sortlist    
    return sortlist
